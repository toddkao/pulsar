/Matrix';\r\nvar Triangle = /** @class */ (function () {\r\n    function Triangle(a, b, c) {\r\n        this.a = a;\r\n        this.b = b;\r\n        this.c = c;\r\n        this.id = uniqueId();\r\n        var ab = new Line(a, b);\r\n        var bc = new Line(b, c);\r\n        var ca = new Line(c, a);\r\n        this.lines = { ab: ab, bc: bc, ca: ca };\r\n    }\r\n    Object.defineProperty(Triangle.prototype, \"centroid\", {\r\n        get: function () {\r\n            return Vector.FindPolyCentroid(this.points);\r\n        },\r\n        enumerable: true,\r\n        configurable: true\r\n    });\r\n    Object.defineProperty(Triangle.prototype, \"points\", {\r\n        get: function () {\r\n            return [this.a, this.b, this.c];\r\n        },\r\n        enumerable: true,\r\n        configurable: true\r\n    });\r\n    Object.defineProperty(Triangle.prototype, \"linesArray\", {\r\n        get: function () {\r\n            return [this.lines.ab, this.lines.bc, this.lines.ca];\r\n        },\r\n        enumerable: true,\r\n        configurable: true\r\n    });\r\n    Triangle.prototype.equals = function (triangle) {\r\n        var _a = this.lines, ab = _a.ab, bc = _a.bc, ca = _a.ca;\r\n        var sameAB = ab.equals(triangle.lines.ab) ||\r\n            ab.equals(triangle.lines.bc) ||\r\n            ab.equals(triangle.lines.ca);\r\n        var sameBC = bc.equals(triangle.lines.ab) ||\r\n            bc.equals(triangle.lines.bc) ||\r\n            bc.equals(triangle.lines.ca);\r\n        var sameCA = ca.equals(triangle.lines.ab) ||\r\n            ca.equals(triangle.lines.bc) ||\r\n            ca.equals(triangle.lines.ca);\r\n        return sameAB && sameBC && sameCA;\r\n    };\r\n    Triangle.prototype.isPointInCircumcircle = function (point) {\r\n        var ax = this.a.x;\r\n        var ay = this.a.y;\r\n        var bx = this.b.x;\r\n        var by = this.b.y;\r\n        var cx = this.c.x;\r\n        var cy = this.c.y;\r\n        var a = ax;\r\n        var b = ay;\r\n        var c = ax * ax + ay * ay;\r\n        var d = 1;\r\n        var e = bx;\r\n        var f = by;\r\n        var g = bx * bx + by * by;\r\n        var h = 1;\r\n        var i = cx;\r\n        var j = cy;\r\n        var k = cx * cx + cy * cy;\r\n        var l = 1;\r\n        var m = point.x;\r\n        var n = point.y;\r\n        var o = point.x * point.x + point.y * point.y;\r\n        var p = 1;\r\n        var matrix = new Matrix4(a, b, c, d, e, f, g, h, i, j, k, l, m, n, o, p);\r\n        return matrix.determine() < 0;\r\n    };\r\n    Triangle.prototype.hasPoint = function (point) {\r\n        return this.a.equals(point) || this.b.equals(point) || this.c.equals(point);\r\n    };\r\n    Triangle.prototype.hasAnyPoint = function (points) {\r\n        var _this = this;\r\n        return (points.filter(function (point) {\r\n            return _this.hasPoint(point);\r\n        }).length !== 0);\r\n    };\r\n    Triangle.LinesFromArray = function (triangles) {\r\n        return triangles.reduce(function (accumulator, triangle) {\r\n            accumulator.push.apply(accumulator, triangle.linesArray);\r\n            return accumulator;\r\n        }, []);\r\n    };\r\n    Triangle.GetUniqueLines = function (triangles) {\r\n        var lines = Triangle.LinesFromArray(triangles);\r\n        return lines.filter(function (line) { return Line.IsUnique(line, lines); });\r\n    };\r\n    return Triangle;\r\n}());\r\nexport default Triangle;\r\n//# sourceMappingURL=Triangle.js.map","dtsmap":{"name":"/Users/radu.milici/Documents/git/pulsar-pathfinding/common/Triangle.d.ts.map","writeByteOrderMark":false,"text":"{\"version\":3,\"file\":\"Triangle.d.ts\",\"sourceRoot\":\"\",\"sources\":[\"../src/common/Triangle.ts\"],\"names\":[],\"mappings\":\"AAAA,OAAO,MAAM,MAAM,UAAU,CAAC;AAC9B,OAAO,IAAI,MAAM,QAAQ,CAAC;AAC1B,OAAO,EAAE,EAAE,EAAE,MAAM,eAAe,CAAC;AAInC,aAAK,aAAa,GAAG;IAAE,EAAE,EAAE,IAAI,CAAC;IAAC,EAAE,EAAE,IAAI,CAAC;IAAC,EAAE,EAAE,IAAI,CAAA;CAAE,CAAC;AAEtD,MAAM,CAAC,OAAO,eAAgB,