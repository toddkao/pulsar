 this.entities.forEach(function (entity) { return entity.stop(); });\r\n    };\r\n    EntityUpdater.prototype.clear = function () {\r\n        this.entities.length = 0;\r\n    };\r\n    EntityUpdater.prototype.add = function (entity) {\r\n        var _this = this;\r\n        entity.updater = this.updater;\r\n        this.entities.push(entity);\r\n        var callback = function (component) {\r\n            component.entity = entity;\r\n            return _this.updater.addComponent(component);\r\n        };\r\n        return this.loopComponents(entity.components, callback);\r\n    };\r\n    EntityUpdater.prototype.remove = function (_a) {\r\n        var _this = this;\r\n        var components = _a.components;\r\n        var callback = function (component) {\r\n            return _this.updater.removeComponent(component);\r\n        };\r\n        return this.loopComponents(components, callback);\r\n    };\r\n    EntityUpdater.prototype.toggle = function (_a) {\r\n        var _this = this;\r\n        var components = _a.components;\r\n        var callback = function (component) {\r\n            return _this.updater.toggleComponent(component);\r\n        };\r\n        return this.loopComponents(components, callback);\r\n    };\r\n    EntityUpdater.prototype.loopComponents = function (components, callback) {\r\n        return components.map(function (component) {\r\n            return {\r\n                id: component.id,\r\n                name: component.name,\r\n                success: callback(component),\r\n            };\r\n        });\r\n    };\r\n    return EntityUpdater;\r\n}());\r\nexport default EntityUpdater;\r\n//# sourceMappingURL=EntityUpdater.js.map","dtsmap":{"name":"/Users/radu.milici/Documents/git/pulsar-pathfinding/ecs/Updater/EntityUpdater.d.ts.map","writeByteOrderMark":false,"text":"{\"version\":3,\"file\":\"EntityUpdater.d.ts\",\"sourceRoot\":\"\",\"sources\":[\"../../src/ecs/Updater/EntityUpdater.ts\"],\"names\":[],\"mappings\":\"AAAA,OAAO,MAAM,M