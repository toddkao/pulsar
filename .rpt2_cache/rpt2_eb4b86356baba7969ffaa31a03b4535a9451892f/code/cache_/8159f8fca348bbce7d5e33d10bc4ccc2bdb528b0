       var x = this.getX();\r\n            var y = this.getY();\r\n            return new Vector({ x: x, y: y });\r\n        },\r\n        enumerable: true,\r\n        configurable: true\r\n    });\r\n    LineIntersection.prototype.getX = function () {\r\n        var a = new Matrix2(this.x1, this.y1, this.x2, this.y2);\r\n        var b = new Matrix2(this.x1, 1, this.x2, 1);\r\n        var c = new Matrix2(this.x3, this.y3, this.x4, this.y4);\r\n        var d = new Matrix2(this.x3, 1, this.x4, 1);\r\n        var abcd = new Matrix2(a.determine(), b.determine(), c.determine(), d.determine());\r\n        return abcd.determine() / this.efghDeterminant;\r\n    };\r\n    LineIntersection.prototype.getY = function () {\r\n        var a = new Matrix2(this.x1, this.y1, this.x2, this.y2);\r\n        var b = new Matrix2(this.y1, 1, this.y2, 1);\r\n        var c = new Matrix2(this.x3, this.y3, this.x4, this.y4);\r\n        var d = new Matrix2(this.y3, 1, this.y4, 1);\r\n        var abcd = new Matrix2(a.determine(), b.determine(), c.determine(), d.determine());\r\n        return abcd.determine() / this.efghDeterminant;\r\n    };\r\n    LineIntersection.prototype.isOnSegments = function () {\r\n        var a = new Matrix2(this.x1 - this.x3, this.x3 - this.x4, this.y1 - this.y3, this.y3 - this.y4);\r\n        var b = new Matrix2(this.x1 - this.x2, this.x3 - this.x4, this.y1 - this.y2, this.y3 - this.y4);\r\n        var c = new Matrix2(this.x1 - this.x2, this.x1 - this.x3, this.y1 - this.y2, this.y1 - this.y3);\r\n        var d = new Matrix2(this.x1 - this.x2, this.x3 - this.x4, this.y1 - this.y2, this.y3 - this.y4);\r\n        var divisionAB = a.determine() / b.determine();\r\n        var divisionCD = -(c.determine() / d.determine());\r\n        var isOnSegmentA = divisionAB >= 0 && divisionAB <= 1;\r\n        var isOnSegmentB = divisionCD >= 0 && divisionCD <= 1;\r\n        return isOnSegmentA && isOnSegmentB;\r\n    };\r\n    return LineIntersection;\r\n}());\r\nexport default Lin