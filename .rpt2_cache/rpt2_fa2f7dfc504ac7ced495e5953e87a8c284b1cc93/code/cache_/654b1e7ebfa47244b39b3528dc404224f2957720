;\r\n        const callback = (component) => {\r\n            component.entity = entity;\r\n            return this.updater.addComponent(component);\r\n        };\r\n        return this.loopComponents(entity.components, callback);\r\n    }\r\n    remove({ components }) {\r\n        const callback = component => this.updater.removeComponent(component);\r\n        return this.loopComponents(components, callback);\r\n    }\r\n    toggle({ components }) {\r\n        const callback = component => this.updater.toggleComponent(component);\r\n        return this.loopComponents(components, callback);\r\n    }\r\n    loopComponents(components, callback) {\r\n        return components.map((component) => {\r\n            return {\r\n                id: component.id,\r\n                name: component.name,\r\n                success: callback(component),\r\n            };\r\n        });\r\n    }\r\n}\r\n//# sourceMappingURL=EntityUpdater.js.map","dtsmap":{"name":"/Users/radu.milici/Documents/git/pulsar-pathfinding/ecs/Updater/EntityUpdater.d.ts.map","writeByteOrderMark":false,"text":"{\"version\":3,\"file\":\"EntityUpdater.d.ts\",\"sourceRoot\":\"\",\"sources\":[\"../../src/ecs/Updater/EntityUpdater.ts\"],\"names\":[],\"mappings\":\"AAAA,OAAO,MAAM,MAAM,WAAW,CAAC;AAE/B,OAAO,OAAO,MAAM,WAAW,CAAC;AAChC,OAAO,EAAE,aAAa,EAAE,MAAM,kBAAkB,CAAC;AAIjD,MAAM,CAAC,OAAO;IAGA,OAAO,CAAC,QAAQ,CAAC,OAAO;IAFpC,OAAO,CAAC,QAAQ,CAAC,QAAQ,CAAgB;gBAEZ,OAAO,EAAE,OAAO;IAE7C,KAAK,IAAI,IAAI;IAIb,IAAI,IAAI,IAAI;IAIZ,KAAK,IAAI,IAAI;IAIb,GAAG,CAAC,MAAM,EAAE,MAAM,GAAG,aAAa,EAAE;IAUpC,MAAM,CAAC,EAAE,UAAU,EAAE,EAAE,MAAM,GAAG,aAAa,EAAE;IAM/C,MAAM,CAAC,EAAE,UAAU,EAAE,EAAE,MAAM,GAAG,aAAa,EAAE;IAM/C,OAAO,CAAC,cAAc;CAYvB\"}"},"dts":{"name":"/Users/radu.milici/Documents/git/pulsar-pathfinding/ecs/Updater/EntityUpdater.d.ts","text":"import Entity from '../Entity';\r\nimport Updater from './Updater';\r\nimport { updaterReport } from '../../interfaces';\r\nexport default class EntityUpdater {\r\n    private readonly updater;\r\n    private readonly entities;\r\n    constructor(updater: Updater);\r\n    start(): void;\r\n    stop(): void;\r\n    clear(): void;\r\n    add(entity: Entity): updaterReport[];\r\n    remove({ components }: Entity): updaterReport[];\r\n    toggle({ components }: Entity): updaterReport[];\r\n    private loopComponents;\r\n}\r\n//# sourceMappingURL=EntityUpdater.d.ts.map"}}
