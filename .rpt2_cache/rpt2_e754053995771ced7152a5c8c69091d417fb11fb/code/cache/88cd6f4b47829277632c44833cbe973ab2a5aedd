}\r\n//# sourceMappingURL=Hull.js.map","dtsmap":{"name":"/Users/radu.milici/Documents/git/pulsar-pathfinding/triangulation/Hull.d.ts.map","writeByteOrderMark":false,"text":"{\"version\":3,\"file\":\"Hull.d.ts\",\"sourceRoot\":\"\",\"sources\":[\"../src/triangulation/Hull.ts\"],\"names\":[],\"mappings\":\"AAAA,OAAO,EAAE,MAAM,EAAE,IAAI,EAAY,MAAM,WAAW,CAAC;AACnD,OAAO,aAAa,MAAM,iBAAiB,CAAC;AAE5C,MAAM,CAAC,OAAO;IACZ,QAAQ,CAAC,KAAK,EAAE,IAAI,EAAE,CAAM;IAC5B,OAAO,CAAC,OAAO,CAAW;IAC1B,OAAO,CAAC,QAAQ,CAAC,SAAS,CAAa;gBAE3B,EAAE,SAAS,EAAE,EAAE,aAAa;aAIpC,MAAM,EAAI,MAAM,EAAE;IAItB,KAAK,IAAI,IAAI;CAiBd\"}"},"dts":{"name":"/Users/radu.milici/Documents/git/pulsar-pathfinding/triangulation/Hull.d.ts","text":"import { Vector, Line } from '../common';\r\nimport Triangulation from './Triangulation';\r\nexport default class Hull {\r\n    readonly lines: Line[];\r\n    private _points;\r\n    private readonly triangles;\r\n    constructor({ triangles }: Triangulation);\r\n    readonly points: Vector[];\r\n    start(): void;\r\n}\r\n//# sourceMappingURL=Hull.d.ts.map"}}
